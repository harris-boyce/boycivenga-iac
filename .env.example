# NetBox API Configuration
# Copy this file to .env and fill in your values
# Never commit the .env file to version control!

# NetBox API endpoint URL
# For local development (default):
NETBOX_URL=http://localhost:8000/api/

# For remote NetBox instance:
# NETBOX_URL=https://netbox.example.com/api/

# NetBox API Token (REQUIRED)
# Generate this from NetBox UI: Admin -> Users -> Tokens
NETBOX_API_TOKEN=your-token-here
